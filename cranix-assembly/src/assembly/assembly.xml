            <directory>${project.basedir}/../cranix-api/target/</directory>
            <outputDirectory></outputDirectory>
            <includes>
                <include>cranix-api-4.2-with-dependencies.jar</include>
            </includes>
        </fileSet>
    </fileSets>


</assembly>
